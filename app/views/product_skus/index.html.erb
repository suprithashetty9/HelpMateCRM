<p id="notice"><%= notice %></p>

<h1>Product Skus</h1>

<table>
  <thead>
    <tr>
      <th>Productsku</th>
      <th>Createdby</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_skus.each do |product_sku| %>
      <tr>
        <td><%= product_sku.productsku %></td>
        <td><%= product_sku.createdby %></td>
        <td><%= product_sku.product %></td>
        <td><%= link_to 'Show', product_sku %></td>
        <td><%= link_to 'Edit', edit_product_sku_path(product_sku) %></td>
        <td><%= link_to 'Destroy', product_sku, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Sku', new_product_sku_path %>
